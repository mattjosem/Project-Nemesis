<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.connections.sharanjit.ConnectionListFragment">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/connectionGuiList"
        android:name="edu.uw.tcss450.ui.ConnectionFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="30dp"
        tools:context=".ui.ConnectionFragment"
        tools:listitem="@layout/fragment_connection">

    </androidx.recyclerview.widget.RecyclerView>

</FrameLayout>